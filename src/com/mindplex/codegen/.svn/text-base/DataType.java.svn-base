package com.mindplex.codegen;

/**
 *
 *  @author Abel Perez
 */
public enum DataType
{
    SHORT ("short"),
    INT ("int"),
    LONG ("long"),
    FLOAT ("float"),
    DOUBLE ("double"),
    DECIMAL ("decimal"),
    BYTE ("byte"),
    CHAR ("char"),
    STRING ("string"),
    TEXT ("text"),
    LONGTEXT ("longtext"),
    BOOLEAN ("bool"),
    DATETIME ("datetime");

    private final String type;

    DataType(String type) {
        this.type = type;    
    }

    String getValue() {
        return type;
    }
}
