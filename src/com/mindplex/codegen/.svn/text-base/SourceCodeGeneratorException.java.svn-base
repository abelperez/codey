package com.mindplex.codegen;

/**
 *
 * @author Abel Perez
 */
public class SourceCodeGeneratorException extends Exception
{
    public SourceCodeGeneratorException() {
        super();
    }

    public SourceCodeGeneratorException(String message) {
        super(message);
    }

    public SourceCodeGeneratorException(String message, Throwable cause) {
        super(message, cause);
    }

    public SourceCodeGeneratorException(Throwable cause) {
        super(cause);
    }
}